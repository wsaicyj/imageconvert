{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://www.imageconvert.me/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "destination": "https://www.imageconvert.me/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "imageconvert.me"
        }
      ]
    }
  ]
}